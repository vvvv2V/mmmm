{
  "name": "limpezapro-backend",
  "version": "1.0.0",
  "description": "Backend Express para plataforma de limpeza",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "node src/db/runMigrations.js",
    "seed": "node src/db/seed.js",
    "db:init": "node scripts/initDb.js",
    "queue:worker": "node src/workers/emailQueueWorker.js",
    "queue:worker:watch": "nodemon src/workers/emailQueueWorker.js",
    "queue:clean": "node -e \"require('./src/services/EmailQueueService.js').cleanFailedJobs().then(() => process.exit(0))\"",
    "queue:stats": "node -e \"require('./src/services/EmailQueueService.js').getQueueStats().then(s => console.log(JSON.stringify(s, null, 2))).then(() => process.exit(0))\""
  },
  "dependencies": {
    "@sentry/node": "^7.84.0",
    "axios": "^1.3.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "bull-board": "^1.7.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^12.7.0",
    "googleapis": "^118.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^11.4.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^7.0.13",
    "openai": "^6.17.0",
    "pg": "^8.8.0",
    "redis": "^4.6.5",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.3",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.0.2",
    "stripe": "^11.8.0",
    "twilio": "^5.12.0",
    "web-push": "^3.5.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "eslint": "^9.39.2",
    "jest": "^29.3.1",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3"
  }
}
