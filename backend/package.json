{
  "name": "limpezapro-backend",
  "version": "1.0.0",
  "description": "Backend Express para plataforma de limpeza",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "node src/db/runMigrations.js",
    "seed": "node src/db/seed.js",
    "db:init": "node scripts/initDb.js",
    "queue:worker": "node src/workers/emailQueueWorker.js",
    "queue:worker:watch": "nodemon src/workers/emailQueueWorker.js",
    "queue:clean": "node -e \"require('./src/services/EmailQueueService.js').cleanFailedJobs().then(() => process.exit(0))\"",
    "queue:stats": "node -e \"require('./src/services/EmailQueueService.js').getQueueStats().then(s => console.log(JSON.stringify(s, null, 2))).then(() => process.exit(0))\"",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "2fa:setup": "node -e \"const TwoFactorService = require('./backend/src/services/TwoFactorService.js'); console.log(JSON.stringify(TwoFactorService.generateTOTPSecret('test@example.com'), null, 2))\"",
    "invoices:cleanup": "node -e \"const InvoiceService = require('./backend/src/services/InvoiceService.js'); InvoiceService.deleteOldInvoices(30).then(() => process.exit(0))\"",
    "invoices:stats": "node -e \"const InvoiceService = require('./backend/src/services/InvoiceService.js'); console.log(JSON.stringify(InvoiceService.getStats(), null, 2))\"",
    "crypto:generate-key": "node -e \"const crypto = require('crypto'); console.log('Chave 256-bit (hex):', crypto.randomBytes(32).toString('hex'))\"",
    "crypto:audit": "node -e \"const ChatEncryptionService = require('./backend/src/services/ChatEncryptionService.js'); console.log(ChatEncryptionService)\"",
    "migration:chat": "node src/database/migrations/002_create_chat_messages_table.js",
    "db:report": "node -e \"const DatabaseOptimizationService = require('./backend/src/services/DatabaseOptimizationService.js'); console.log(JSON.stringify(DatabaseOptimizationService.getQueryReport(), null, 2))\"",
    "db:slow-queries": "node -e \"const DatabaseOptimizationService = require('./backend/src/services/DatabaseOptimizationService.js'); DatabaseOptimizationService.detectSlowQueries(null, 100).then(q => console.log(JSON.stringify(q, null, 2))).then(() => process.exit(0))\"",
    "db:indices": "node -e \"const DatabaseOptimizationService = require('./backend/src/services/DatabaseOptimizationService.js'); console.log(JSON.stringify(DatabaseOptimizationService.suggestIndices(), null, 2))\"",
    "cdn:report": "node -e \"const CDNAssetOptimizerService = require('./backend/src/services/CDNAssetOptimizerService.js'); console.log(JSON.stringify(CDNAssetOptimizerService.generateOptimizationReport(), null, 2))\"",
    "cdn:savings": "node -e \"const CDNAssetOptimizerService = require('./backend/src/services/CDNAssetOptimizerService.js'); console.log(JSON.stringify(CDNAssetOptimizerService.calculateBandwidthSavings(), null, 2))\""
  },
  "dependencies": {
    "@bull-board/api": "^6.16.4",
    "@bull-board/express": "^6.16.4",
    "@sentry/node": "^7.84.0",
    "axios": "^1.3.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "bull-board": "^1.7.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^12.7.0",
    "googleapis": "^118.0.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "newrelic": "^11.4.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^7.0.13",
    "openai": "^6.17.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.8.0",
    "qrcode": "^1.5.4",
    "redis": "^4.6.5",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.3",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.0.2",
    "stripe": "^11.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.12.1",
    "web-push": "^3.5.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@playwright/test": "^1.58.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "eslint": "^9.39.2",
    "jest": "^29.3.1",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3"
  }
}
